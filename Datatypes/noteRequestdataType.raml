#%RAML 1.0 DataType
type: object
displayName: NoteRequest
description: Data required to create or update a note

properties:
  title:
    type: string
    required: true
    example: "ABC"

  content:
    type: string
    required: true
    example: "Note Description ........."

  createdAt:
    type: datetime
    required: true
    example: 2025-01-01T10:00:00Z

  updatedAt:
    type: datetime
    required: true
    example: 2025-01-02T10:00:00Z
